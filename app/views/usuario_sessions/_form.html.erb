<%= form_tag usuario_sessions_path, :method => :post do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= label_tag :remember_me %>
  <%= check_box_tag :remember_me, 1, params[:remember_me] %>
  <%= submit_tag "Login" %>
<% end %>



